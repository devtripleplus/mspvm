[www]
user = www-data
group = www-data
listen = 127.0.0.1:9000
pm = dynamic
pm.max_children = 20
pm.start_servers = 2
pm.min_spare_servers = 2
pm.max_spare_servers = 2
pm.process_idle_timeout = 3s;
pm.max_requests = 100;
chdir = /var/
env[HOSTNAME] = \$HOSTNAME
php_admin_value[upload_max_filesize] = 32M
